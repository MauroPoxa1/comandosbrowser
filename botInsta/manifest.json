{
"name": "Bot Insta Pro",
"description": "Link to G Suite Certification website",
"version": "1.1",
"manifest_version": 3,
"permissions": ["scripting", "activeTab","storage","webRequest"],
"icons": {
"128": "123.png"
},
"action": {
    "default_popup": "index.html"
},
 "commands": {
    "_execute_action": {
    }
 },
 "content_scripts": [ {
    "all_frames": true,
    "js": [ "inject.js" ],
    "match_about_blank": true,
    "matches": [ "http://www.instagram.com/*", "https://www.instagram.com/*" ],
    "run_at": "document_start"
 } ]
}